<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
   
        <referenceBlock name="customer_account_navigation">
            <block class="Magento\Customer\Block\Account\Delimiter" name="customer-account-navigation-delimiter-cp" template="Magento_Customer::account/navigation-delimiter.phtml">
                 <arguments>
                         <argument name="sortOrder" xsi:type="number">30</argument>
                </arguments>
            </block>
            <block class="Magento\Customer\Block\Account\SortLinkInterface"  name="cp_manage_staffs">
                <arguments>
                    <argument name="path" xsi:type="string">company/staff</argument>
                    <argument name="label" xsi:type="string" translate="true">Manage Staff</argument>
                    <argument name="sortOrder" xsi:type="number">30</argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>
